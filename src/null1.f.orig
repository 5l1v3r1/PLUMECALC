!  assumes any liability or responsibility for the use of this software.
!***********************************************************************
!  
!   PURPOSE
!   
!   Check for null lines or 0's in lines.
!  
!   Developed originally for FEHM.
!    
!***********************************************************************
!
! INTERFACES
!
! Formal Calling Parameters
!
!   Identifier      Type     Use  Description
!
!   wd              CHAR     I   String whose content is to be checked
!
! Interface Tables
!
!   None
!
! Files
!
!   None
!
!***********************************************************************
!
! LOCAL IDENTIFIERS
!
! Local Constants
!
!   None
!
! Local Types
!
!   None
!
! Local variables
!
!   Identifier      Type     Description
!
!   i               INT      Loop index
!   length          INT      Length of input string
!
! Local Subprograms
!
!   None
!
!***********************************************************************
!
! FUNCTIONAL DESCRIPTION
!
! Check whether a string has characters or is null or zero's. If the
! string has characters return 'FALSE', otherwise if it is null or 
! zero's return 'TRUE'.
!
!***********************************************************************
!
! PSEUDOCODE
!
! BEGIN null1
! 
!   determine the length of the input string
! 
!   FOR each character in the string
!   
!       IF the character is not a ".", " ", ",", "0", or tab
!          set the value to .FALSE. and return
!       ENDIF   
!       
!   ENDFOR
!   
!   set the value to .TRUE. and return
!       
! END null1
!
!***********************************************************************

      implicit none

      integer i,length
      character*(*) wd

      length = len(wd)
      do i = 1, length
         if (wd(i:i) .ne. '.' .and. 
     *       wd(i:i) .ne. ' ' .and. 
     *       wd(i:i) .ne. ',' .and. 
     *       wd(i:i) .ne. '0' .and. 
     *       wd(i:i) .ne. char(9)) then
            null1 = .FALSE.
            return
         endif
      end do

      null1 = .TRUE.

      return
      end
